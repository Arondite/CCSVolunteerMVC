
@{
    ViewBag.Title = "Volunteer Pin";
}

<h2>Volunteer Pin</h2>
<form action="@Url.Action("VolunteerClock", "Kiosk")" method="post" class="form-horizontal" id="login">
	<div class="form-group">
		<label for="volunteerPin" class="col-md-2 col-form-label">Volunteer Pin</label>
		<div class="col-md-10">
			<input class="form-control" type="number" value="42" id="volunteerPin" />
		</div>
		<input class="btn btn-primary btn-lg btn-block" name="VolunteerPin" type="submit" id="VolunteerPin" formmethod="POST" title="VolunteerPin" value="Volunteer Pin">
	</div>
</form>